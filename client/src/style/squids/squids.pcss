.squid {
  text-align: center;
}

.squid__show {
  background-color: #E7D6CD;
  border-radius: 1rem;
  padding: 10px 10px;
  margin: 50px 50px;
  width: auto;
  height: auto;
}

.squid__tile {
  background-color: #D1B1A0;
  border-radius: 1rem;
  padding: 10px 10px;
  margin: 10px 10px;
}

.tile {
  background-color: #F7F2EF;
  border-radius: 1rem;
  padding: 25px 25px;
  margin: 10px 10px;
  font-size: 25px;
  line-height: 32px;
}